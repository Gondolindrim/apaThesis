keywords = {Inverter,Inverter model,Microgrid,Power control,Small-signal stability},
number = {2},
pages = {613--625},
title = {{Modeling, analysis and testing of autonomous operation of an inverter-based microgrid}},
url = {https://www.offshorewind.biz/2017/10/04/chinas-largest-offshore-wind-farm-goes-online/},
