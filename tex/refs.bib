@article{Sadabadi2018,
abstract = {IEEE The purpose of this paper is to explore the applicability of linear time-invariant (LTI) dynamical systems with polytopic uncertainty for modeling and control of islanded DC microgrids under plug-and-play (PnP) functionality of distributed generations (DGs). We develop a robust decentralized voltage control framework to ensure robust stability and reliable operation for islanded DC microgrids. The problem of voltage control of islanded DC microrgids with PnP operation of DGs is formulated as a convex optimization problem with structural constraints on some decision variables. The proposed control scheme offers several advantages including decentralized voltage control with no communication link, transient stability/performance, plug-and-play capability, scalability of design, applicability to microgrids with general topology, and robustness to microgrid uncertainties. The effectiveness of the proposed control approach is evaluated through simulation studies carried out in MATLAB/SimPowerSystems Toolbox.},
author = {Sadabadi, Mahdieh S. and Shafiee, Qobad and Karimi, Alireza},
doi = {10.1109/TSG.2017.2728319},
file = {:home/alvaro/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sadabadi, Shafiee, Karimi - 2018 - Plug-and-play robust voltage control of DC microgrids.pdf:pdf},
issn = {19493053},
journal = {IEEE Transactions on Smart Grid},
keywords = {Convex optimization,DC microgrids,plug-and-play operation,polytopic uncertainty,robust control,voltage control},
number = {6},
pages = {6886--6896},
publisher = {IEEE},
title = {{Plug-and-play robust voltage control of DC microgrids}},
volume = {9},
year = {2018}
}
